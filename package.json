{
  "name": "kuloer",
  "version": "0.1.2",
  "description": "A lightning-fast CSS color parsing and manipulation library with a tiny footprint",
  "license": "MIT",
  "repository": "kasperisager/kuloer",
  "author": "Kasper Kronborg Isager <kasperisager@gmail.com>",
  "scripts": {
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "transpile": "babel lib -d dist",
    "prepublish": "npm run transpile"
  },
  "main": "dist/color.js",
  "jsnext:main": "lib/color.js",
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "functional",
    "css",
    "color",
    "parsing",
    "manipulation",
    "es6",
    "modern",
    "browser"
  ],
  "dependencies": {
    "bundstreg": "^0.3.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.10.1",
    "coveralls": "^2.11.11",
    "nyc": "^6.6.1",
    "xo": "^0.16.0"
  },
  "ava": {
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "xo": {
    "space": true,
    "exnext": true,
    "rules": {
      "no-new": 0,
      "no-self-compare": 0,
      "no-implicit-coercion": 0
    }
  }
}
